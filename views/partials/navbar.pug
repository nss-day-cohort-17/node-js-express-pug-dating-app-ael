nav.navbar.navbar-inverse.navbar-fixed-top
  .container
    .navbar-header
      button.navbar-toggle.collapsed(type='button', data-toggle="collapse", data-target="#navbar")
        span.icon-bar
        span.icon-bar
        span.icon-bar
      a.navbar-brand(href='/') CAREER CUPID

      #navbar.collapse.navbar-collapse
        ul.nav.navbar-nav
          li(class={active: page === 'Home' })
            a(href='/home') Home
          li(class={active: page === 'Liked'})
            a(href='/liked') Liked

          if email
            li: a(href="/logout") Logged in as #{email}
          else
            li(class={ active: page === 'Login' })
              a(href="/login") Login
            li(class={ active: page === 'Register' })
              a(href="/registration") Register
